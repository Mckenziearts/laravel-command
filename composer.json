{
    "name": "mckenziearts/laravel-command",
    "version": "1.0.0",
    "description": "A simple Laravel package to provide artisan new commands",
    "type":"laravel-package",
    "keywords": [
        "generators",
        "laravel",
        "artisan-cli",
        "observer",
        "helper",
        "repository"
    ],
    "homepage": "https://github.com/Mckenziearts/laravel-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Monney",
            "email": "monneylobe@gmail.com",
            "homepage": "http://www.twitter.com/mckenziearts",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": ">=5.1",
        "illuminate/console": ">=5.1",
        "php" : ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Mckenziearts\\LaravelCommand\\": "src"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Mckenziearts\\LaravelCommand\\LaravelCommandServiceProvider"
            ]
        }
    }
}
